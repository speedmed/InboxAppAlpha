<div class="dashboard-page">
  <div class="container-fluid">
    <div class="row">
      <div class="col-sm-3 col-md-2 sidebar">
       
       <div class="text-center">
        <h2 class="brand">MailBox Angularjs<small>Alpha</small></h2>
        <img src="/app/images/flat-avatar.png" class="user-avatar" /><br />
        
      </div><br />

      <div class="text-center"><span class="glyphicon glyphicon-user" aria-hidden="true"></span> <%= user %></div>

      <ul class="nav nav-sidebar">
        <li ng-class="{active: $state.includes('inbox')}"><a ui-sref="inbox">Inbox <span class="sr-only">(current)</span></a></li>
        <li ng-class="{active: $state.includes('sent')}"><a ui-sref="sent">Sent Mail</a></li>
        <li ng-class="{active: $state.includes('drafts')}"><a ui-sref="drafts">Drafts</a></li>
        
      </ul>

    </div>
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main" ui-view>
    </div>
  </div>
</div>
</div>
</div>

